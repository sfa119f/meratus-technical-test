{
  "name": "simple-logistic-app-backend",
  "version": "1.0.0",
  "description": "Simple logistic app backend with Node.js, TypeScript, and Prisma ORM",
  "author": "Syarifuddin",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "dev": "nodemon",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:create-only": "npx prisma migrate dev --create-only",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "run-prettier": "prettier src/*.ts",
    "validate-prettiness": "yarn run-prettier -c",
    "make-prettier": "yarn run-prettier --write"
  },
  "dependencies": {
    "@prisma/client": "6.9.0",
    "@prisma/extension-accelerate": "2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "http-errors": "^1.7.1",
    "morgan": "^1.9.1",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/http-errors": "1.8.2",
    "@types/morgan": "1.9.9",
    "@types/node": "^22.15.32",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "6.5.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.31.0",
    "nodemon": "^3.1.10",
    "prettier": "2.8.8",
    "prisma": "6.9.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  }
}
